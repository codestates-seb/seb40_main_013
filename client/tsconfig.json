{
    "compilerOptions": {
      "jsx": "preserve",
      "allowJs": true,
      "target": "ES5",
      "outDir": "./dist",
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "lib": ["ES2015", "DOM", "DOM.Iterable"],
    },
    "include": ["./src/**/*", "tsconfig.strictNullChecks.json"],
    "exclude": ["node_modules", "dist"],
    "jsoncompilerOptions": {
      "strictNullChecks": true
    }
  }